{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "off",
    "no-prototype-builtins": "off",
    "no-undef": "off",
    "react/no-find-dom-node": "off",
    "no-case-declarations": "off"
  },
  "overrides": []
}
